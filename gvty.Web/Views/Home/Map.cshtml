@{
    Layout = null;
}

<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style>
        html {
            height: 100%
        }
        body {
            height: 100%;
            margin: 0;
            padding: 0;
        }
        #mapDiv {
            height: 100%;
        }
    </style>
    <title></title>
</head>
<body>
    
    <div id="mapDiv"></div>

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDNdTK4h8OHtrdSa8NPy5ldvQ8Pasng-Ns"></script>
    <script type="text/javascript">
        function init() {
            var mapOptions = {
                center: new google.maps.LatLng(0, 0),
                zoom: 4
            };
            var map = new google.maps.Map(document.getElementById("mapDiv"), mapOptions);
            addKmlLayer(map);
        }

        function addKmlLayer(map) {
            var layer = new google.maps.KmlLayer('@Url.Action("Kml", null, null, Request.Url.Scheme)?path=@Request.QueryString["path"]');
            layer.setMap(map);
        }

        google.maps.event.addDomListener(window, "load", init);
    </script>

</body>
</html>







